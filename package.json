{
  "name": "generator-jhipster-tenantview",
  "version": "1.0.0-git",
  "private": true,
  "description": "Description for Generator Jhipster Tenantview",
  "keywords": [
    "yeoman-generator",
    "jhipster-blueprint",
    "jhipster-8"
  ],
  "license": "UNLICENSED",
  "type": "module",
  "bin": {
    "jhipster-tenantview": "cli/cli.cjs"
  },
  "files": [
    "cli",
    "generators"
  ],
  "scripts": {
    "ejslint": "ejslint generators/**/*.ejs",
    "lint": "eslint .",
    "lint-fix": "npm run ejslint && npm run lint -- --fix",
    "prettier-check": "prettier --check \"{,**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java}\"",
    "prettier-format": "prettier --write \"{,**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java}\"",
    "pretest": "npm run prettier-check && npm run lint",
    "test": "vitest run",
    "update-snapshot": "vitest run --update",
    "vitest": "vitest"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "debug": "^4.3.4",
    "generator-jhipster": "8.3.0",
    "glob": "^10.3.10",
    "lodash-es": "^4.17.21",
    "mem-fs": "^4.0.0",
    "mem-fs-editor": "^11.0.0",
    "node-html-parser": "^6.1.12",
    "semver": "^7.6.0"
  },
  "devDependencies": {
    "ejs-lint": "2.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "prettier": "3.2.5",
    "vitest": "1.4.0",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": ">=8.0.0-rc.1"
  },
  "engines": {
    "node": "^18.13.0 || >= 20.6.1"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
