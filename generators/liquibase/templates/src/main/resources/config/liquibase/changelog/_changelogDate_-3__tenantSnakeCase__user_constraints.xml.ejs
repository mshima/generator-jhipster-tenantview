<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity <%- tenant.entityClass %>.
    -->
    <changeSet id="<%- tenant.changelogDate %>-3" author="jhipster">
        <addForeignKeyConstraint baseColumnNames="<%- tenant.entityLowerCase %>_id"
                                 baseTableName="jhi_user"
                                 constraintName="fk_jhi_user_<%- tenant.entityLowerCase %>_id"
                                 referencedColumnNames="id"
                                 referencedTableName="<%- tenant.entityLowerCase %>"/>

        <addNotNullConstraint   columnName="<%- tenant.entityLowerCase %>_id"
                                columnDataType="bigint"
                                tableName="jhi_user"/>
    </changeSet>
</databaseChangeLog>
